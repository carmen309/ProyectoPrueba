@model IEnumerable<Opositae.Models.Enlace>
@{

    Layout = null; //Para que no pille el general
}

    <div data-wow-delay="0.4s" data-wow-duration="1s" class="row why-choose-us-wrapper wow zoomIn">
        <div class="customs-row">

            @foreach (var item in Model)
            {

                <div class="col-sm-4 col-xs-6 section-icon">
                    <i class="fa @item.claseCss"></i>

                    <p> @item.anteEnlace </p>
                </div>


            }
        </div>
    </div>



